

#include<iostream>


#include<iomanip>//for manipulator setw()


using namespace std;



int main(){


int a=4, b=500;


//Manipulators


//Two Manipulators setw(), endl.


cout<<"Value of a is "<<a<<endl<<"Value of b is "<<b<<endl<<endl;


cout<<"Value of a is "<<setw(3)<<a<<endl<<"Value of b is "<<b;


return 0;


}
