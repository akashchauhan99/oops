#include<iostream>
using namespace std;

main(){
    int a = 10,b = 5;
    cout<<(a&b)<<endl;
    /*
    1010
    0101
    0000
    */
    cout<<(a|b)<<endl;
    /*
    1010
    0101
    1111
    */

    cout<<(~a)<<endl;

    cout<<(a >> 2)<<endl<<(b << 1);
    /*
    1010 = 0010
    0101 = 1010
    */

}
